{
  "hosting": [
    {
      "public": "build",
      "site": "portolab",
      "redirects": [
        {
          "source": "**",
          "destination": "https://pmt-official.web.app",
          "type": 301
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "public": "build",
      "site": "pmt-official",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
